#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kimhabork/Downloads/DIGIHub/sdk/node_modules/.pnpm/@bundlr-network+client@0.11.17/node_modules/@bundlr-network/client/build/esm/node/node_modules:/Users/kimhabork/Downloads/DIGIHub/sdk/node_modules/.pnpm/@bundlr-network+client@0.11.17/node_modules/@bundlr-network/client/build/esm/node_modules:/Users/kimhabork/Downloads/DIGIHub/sdk/node_modules/.pnpm/@bundlr-network+client@0.11.17/node_modules/@bundlr-network/client/build/node_modules:/Users/kimhabork/Downloads/DIGIHub/sdk/node_modules/.pnpm/@bundlr-network+client@0.11.17/node_modules/@bundlr-network/client/node_modules:/Users/kimhabork/Downloads/DIGIHub/sdk/node_modules/.pnpm/@bundlr-network+client@0.11.17/node_modules/@bundlr-network/node_modules:/Users/kimhabork/Downloads/DIGIHub/sdk/node_modules/.pnpm/@bundlr-network+client@0.11.17/node_modules:/Users/kimhabork/Downloads/DIGIHub/sdk/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kimhabork/Downloads/DIGIHub/sdk/node_modules/.pnpm/@bundlr-network+client@0.11.17/node_modules/@bundlr-network/client/build/esm/node/node_modules:/Users/kimhabork/Downloads/DIGIHub/sdk/node_modules/.pnpm/@bundlr-network+client@0.11.17/node_modules/@bundlr-network/client/build/esm/node_modules:/Users/kimhabork/Downloads/DIGIHub/sdk/node_modules/.pnpm/@bundlr-network+client@0.11.17/node_modules/@bundlr-network/client/build/node_modules:/Users/kimhabork/Downloads/DIGIHub/sdk/node_modules/.pnpm/@bundlr-network+client@0.11.17/node_modules/@bundlr-network/client/node_modules:/Users/kimhabork/Downloads/DIGIHub/sdk/node_modules/.pnpm/@bundlr-network+client@0.11.17/node_modules/@bundlr-network/node_modules:/Users/kimhabork/Downloads/DIGIHub/sdk/node_modules/.pnpm/@bundlr-network+client@0.11.17/node_modules:/Users/kimhabork/Downloads/DIGIHub/sdk/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@bundlr-network/client/build/esm/node/cli.js" "$@"
else
  exec node  "$basedir/../@bundlr-network/client/build/esm/node/cli.js" "$@"
fi
